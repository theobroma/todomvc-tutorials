{
  "name": "mobx-reactts-todomvc",
  "version": "0.1.0",
  "private": false,
  "license": "ISC",
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "clsx": "^1.2.1",
    "mobx": "^6.6.2",
    "mobx-react": "^7.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "todomvc-app-css": "^2.4.2",
    "typescript": "^4.8.3",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,yml,json,css,scss,md}",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "serve": "serve -s build",
    "json:server": "json-server -p 4000 --delay 300 --watch db.json --routes routes.json",
    "dev": "concurrently \"npm run json:server\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.x",
    "eslint": "^8.x",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "json-server": "^0.17.0",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": "^16.x"
  }
}
